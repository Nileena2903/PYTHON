    "#GCD of two numbers\n",
    "\n",
    "x=float(input(\"Enter first number :\"))\n",
    "y=float(input(\"Enter second number :\"))\n",
    "if(x>y):\n",
    "    c=x\n",
    "else:\n",
    "    c=y\n",
    "while(c>0):\n",
    "    if(x%c==0)and(y%c==0):\n",
    "        print(\"GCD(\",x,\",\",y,\") =\",c)\n",
    "        break;\n",
    "    c=c-1 "
