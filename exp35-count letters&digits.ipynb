    "#count numbers of letters and digits\n",
    "\n",
    "x=input(\"enter a string :\")\n",
    "a=b=0\n",
    "for c in x:\n",
    "    if(c.isalpha()):\n",
    "        a=a+1\n",
    "    elif(c.isdigit()):\n",
    "        b=b+1\n",
    "    else:\n",
    "        pass\n",
    "print(\"Number of letters=\",a)\n",
    "print(\"Numbers of digits=\",b)"
