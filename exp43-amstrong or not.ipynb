    "# check amstrong or not\n",
    "x=input(\"enter a number :\")\n",
    "result=0\n",
    "for i in range(len(x)):\n",
    "    y=int(x[i])\n",
    "    result=result+(y**int(len(x)))\n",
    "if result==int(x):\n",
    "    print(x,\" is an Amstrong Number\")\n",
    "elif result!=int(x):\n",
    "    print(x,\" is Not an Amstrong Number\")\n"
