   "source": [
    "#52.Store a list of first names.Count the occurrences of ‘a’ within the list.\n",
    "\n",
    "names = [\"alice\", \"Bob\", \"Charlie\", \"Dave\", \"Eve\", \"Frank\"]\n",
    "print(\"list of names :\",names)   \n",
    "count = 0\n",
    "for name in names:\n",
    "    count += name.count(\"a\")\n",
    "    \n",
    "print(f\"\\nNumber of occurrences of 'a': {count}\")"
