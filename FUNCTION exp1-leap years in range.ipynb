   "source": [
    "#print leap years in given range\n",
    "\n",
    "a=int(input(\"Enter starting year :\"))\n",
    "b=int(input(\"Enter last year :\"))\n",
    "print(\"leap years from\",a,\"to\",b,\"is :\")\n",
    "for i in range(a,b):\n",
    "    if(i%4==0)and(i%100!=0)or(i%400==0):\n",
    "        print(i)"
